{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bulma@0.7.2",
      "destination": "wwwroot/lib/bulma/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery-validate@1.18.0",
      "destination": "wwwroot/lib/jquery-validate/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "unpkg",
      "library": "font-awesome-5-css@5.4.1",
      "destination": "wwwroot/lib/font-awesome-5-css/",
      "files": [
        "webfonts/fa-brands-400.eot",
        "webfonts/fa-brands-400.svg",
        "webfonts/fa-brands-400.ttf",
        "webfonts/fa-brands-400.woff",
        "webfonts/fa-brands-400.woff2",
        "webfonts/fa-regular-400.eot",
        "webfonts/fa-regular-400.svg",
        "webfonts/fa-regular-400.ttf",
        "webfonts/fa-regular-400.woff",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-solid-900.eot",
        "webfonts/fa-solid-900.svg",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff",
        "webfonts/fa-solid-900.woff2",
        "css/all.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "datatables-bulma@1.0.1",
      "destination": "wwwroot/lib/datatables-bulma/"
    },
    {
      "library": "datatables@1.10.19",
      "destination": "wwwroot/lib/datatables/",
      "files": [
        "js/jquery.dataTables.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery-datetimepicker@2.5.20",
      "destination": "wwwroot/lib/jquery-datetimepicker/"
    }
  ]
}